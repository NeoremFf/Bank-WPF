<Window x:Class="BankAppGUI.FindAccountAtId"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BankAppGUI"
        mc:Ignorable="d"
        Title="Найти счет" MinHeight="160" MinWidth="200" Height="160" Width="200" Background="#696969">
    <Grid>
        <StackPanel>
            <TextBlock Margin="8,0,0,0">Введите Id счета:</TextBlock>
            <TextBox x:Name="textBoxIdAccount" Height="50" Margin="5"></TextBox>
            <Button Click="GetId"
                Background="#A9A9A9" Margin="7"
                Foreground="White" FontSize="20" >
                Найти
            </Button>
        </StackPanel>
    </Grid>
</Window>
